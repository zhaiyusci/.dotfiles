# BEGIN_KITTY_THEME
# Catppuccin-Frappe
include current-theme.conf
# END_KITTY_THEME
#
font_family      family='JuliaMono Nerd Font Mono' style=Medium features='cv02=1 cv03=1'
bold_font        family='JuliaMono Nerd Font Mono' style=ExtraBold features='cv02=1 cv03=1'
italic_font      family='JuliaMono Nerd Font Mono' style='Medium Italic' features='cv02=1 cv03=1'
bold_italic_font family='JuliaMono Nerd Font Mono' style='ExtraBold Italic' features='cv02=1 cv03=1'

font_size 10.0

disable_ligatures always
# modify_font cell_height 90%

# font_family      Iosevka Fixed
