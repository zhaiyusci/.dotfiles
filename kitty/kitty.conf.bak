# bold_italic_font Victor Mono Bold Italic
#
# font_family      JetBrainsMono Nerd Font
# bold_font        auto
# italic_font      auto
# bold_italic_font auto
#


map ctrl+shift+d new_tab_with_cwd
tab_bar_min_tabs 0
