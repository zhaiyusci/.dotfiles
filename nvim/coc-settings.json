{
  "clangd.fallbackFlags": [
    "-xc++"
  ],
  "clangd.arguments": [
    "--header-insertion=never"
  ],
  "Lua.telemetry.enable": false,
  "julia.trace.server": "verbose",
  "languageserver": {
    "fortran": {
      "command": "fortls",
      "filetypes": [
        "fortran"
      ],
      "rootPatterns": [
        ".fortls",
        ".git/"
      ]
    }
  },
  "sumneko-lua.enableNvimLuaDev": true
}
